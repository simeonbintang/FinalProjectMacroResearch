library(plyr)
merge <- function(a,b,c,d,e,f,g,h,i){
join_all(list(a,b,c,d,e,f), by='date', type='left') %>%
select(-c(g,h)) %>%
mutate(dummy = ifelse(date >= i, 1, 0))
}
