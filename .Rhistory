extract_data <- function (a, b, c, d, e){
temp.file <- paste(tempfile(),".xlsx",sep = "")
download.file(a, temp.file, mode = "wb")
df <- read_excel(temp.file, sheet = b, skip = c)
df <- df[-1,]
colnames(df)[1] <- "date"
df <- df %>% select(c('date', 'Indonesia', 'United States')) %>%
rename({{d}} := Indonesia,
{{e}} := 'United States')
}
