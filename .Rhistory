gdp_indonesia = gdp_indonesia %>% group_by(month=month(date)) %>%
arrange(date) %>%
mutate(growth=value/lag(value,1))%>%
ungroup() %>% arrange(date)
gdp_indonesia$growth<-(gdp_indonesia$growth-1)*100
gdp_indonesia$log_gdp<-log(gdp_indonesia$value)
